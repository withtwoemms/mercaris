<div id="graph" style="width: 400px; height: 200px;">
	<title>Highcharts Example</title>
	<script>
		$(function() {
			$.ajax({
				url: "/dataset",
				success: function(response) {
					console.log(response);
				}
			});
		});
		$(function () { 
		    $('#graph').highcharts({
		        chart: {
		            type: 'bar'
		        },
		        title: {
		            text: 'Fruit Consumption'
		        },
		        xAxis: {
		            categories: ['Apples', 'Bananas', 'Oranges']
		        },
		        yAxis: {
		            title: {
		                text: 'Fruit eaten'
		            }
		        },
		        series: [
			        {
			            name: 'Jane',
			            data: [1, 0, 4]
			        }, 
			        {
			            name: 'John',
			            data: [5, 7, 3]
			        }
			    ]
		    });
		});
	</script>
</div>